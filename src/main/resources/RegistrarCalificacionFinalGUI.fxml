<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="GUI.ControladorRegistrarCalificacionFinalGUI">

   <top>
      <Label text="Asignar Calificación Final" styleClass="header-label">
         <padding>
            <Insets bottom="10" left="10" right="10" top="10"/>
         </padding>
         <font>
            <Font size="24.0"/>
         </font>
      </Label>
   </top>

   <center>
      <TableView fx:id="tablaAsignacion">
         <columns>
            <TableColumn fx:id="columnaMatricula" text="Matrícula" prefWidth="150"/>
            <TableColumn fx:id="columnaNombre" text="Nombre Completo" prefWidth="300"/>
            <TableColumn fx:id="columnaCalificacion" text="Calificación" prefWidth="150"/>
            <TableColumn fx:id="columnaAccion" text="Acción" prefWidth="150"/>
         </columns>
      </TableView>
   </center>
</BorderPane>